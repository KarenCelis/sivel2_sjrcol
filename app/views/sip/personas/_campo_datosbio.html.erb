<% if f.object.datosbio.nil? %>
  <% #f.object.datosbio = Sip::Datosbio.new %>
<% end %>

<%= f.simple_fields_for :datosbio do |d| %>
  <%= d.input :id, as: :hidden %> 
  <%= render partial: 'campo_departamentores', locals: {d: d, f: f} %>
  <%= render partial: 'campo_municipiores', locals: {d: d, f: f} %>
  <%= d.input :veredares%>
  <%= d.input :direccionres %>
  <%= d.input :telefono %>
  <%= d.input :correo %>
  <%= d.input :discapacidad %>
  <%= d.input :cvulnerabilidad_id %>
  <%= render partial: 'campo_escolaridad', locals: {d: d, f: f} %>
  <%= d.input :anioaprobacion %>
  <%= d.input :nivelsisben%>
  <%= d.input :eps %>
  <%= d.input :tipocotizante %>
  <%= d.input :sistemapensional %>
  <%= d.input :afiliadoarl %>
  <%= d.input :subsidioestado %>
  <%= d.input :personashogar %>
  <%= d.input :menores12acargo %>
  <%= d.input :mayores60acargo %>
  <%= d.input :nombreespaciopp %>
  <%= d.input :fechaingespaciopp,
    :input_html => { "data-behaviour" => "datepicker"},
    :as => :string %>

<% end %>
